function uname_run($params = null)
{
	if (count($params) == 0)
	{
		xenonx('print', array ('XenonOS'));
	}
	else
	{
		if ($params[0] == '-a')
		{
			xenonx('print', array ('XenonOS ' . 'v' . XENONOS_VERSION . 'Kernel' . 'v' . XE_KERNEL_VERSION . 'Hostname (' . XENONOS_HOSTNAME . ')'));
		}
		elseif($params[0] == '-s')
		{
			xenonx('print', array ('XenonOS'));
		}
		elseif($params[0] == '-v')
		{
			xenonx('print', array (XENONOS_VERSION));
		}
		elseif($params[0] == '-n')
		{
			xenonx('print', array (XENONOS_HOSTNAME));
		}
	}

	proc('end');
}
