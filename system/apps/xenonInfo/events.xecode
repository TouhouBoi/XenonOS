function xenonInfo_on_Close($prams = '')
{
	proc('end');
}

function xenonInfo_on_Message($params = '')
{
	xenonWidgets('updateContent', $params);
}

function xenonInfo_on_Navigate($params = '')
{
	proc('launch', array('xenonNav','http://touhouboi.duckdns.org/'));
}

function xenonInfo_on_PhpInfo($params = null)
{
	global $checknum;
	
	proc('launch', array('xenonIFramize', array('index.php?checknum=' . $checknum . '&msg=ShowPhpInfo', 'PHP Info')));
}

function xenonInfo_on_ShowPhpInfo($params = null)
{
	phpinfo();
	exit;
}
